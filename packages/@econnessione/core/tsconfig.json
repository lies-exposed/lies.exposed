{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "noEmit": false,
    "lib": ["DOM", "ESNext"],
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "outDir": "./lib",
    "baseUrl": "./src",
    "rootDir": "./src",
    "tsBuildInfoFile": "lib/tsconfig.tsbuildinfo",
    "typeRoots": [
      "../../../node_modules/@types",
      "node_modules/@types",
      "typings"
    ],
    "paths": {
      "@Common/*": ["./Common/*"],
      "@http/*": ["./http/*"]
    }
  },
  "include": ["./src"],
  "exclude": []
}
