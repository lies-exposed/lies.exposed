{
  "name": "@liexp/test",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./lib/*": "./lib/*"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf lib",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "lint": "eslint src",
    "watch": "tsc -b -w"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "src/**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@liexp/core": "workspace:*",
    "fp-ts": "^2.16.6",
    "io-ts": "^2.2.21",
    "io-ts-types": "^0.5.19"
  },
  "devDependencies": {
    "fast-check": "^2.25.0",
    "fast-check-io-ts": "^0.5.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "debug": "^4.3.1"
  }
}
