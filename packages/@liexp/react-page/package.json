{
  "name": "@liexp/react-page",
  "version": "0.1.0",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./lib/*": "./lib/*",
    "./assets/*": "./assets/*"
  },
  "files": [
    "lib",
    "assets"
  ],
  "private": true,
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "test": "vitest"
  },
  "dependencies": {
    "@liexp/shared": "^0.1.0",
    "@react-page/editor": "^5.4.4",
    "@react-page/plugins-background": "^5.4.4",
    "@react-page/plugins-divider": "^5.4.4",
    "@react-page/plugins-html5-video": "^5.4.4",
    "@react-page/plugins-image": "^5.4.4",
    "@react-page/plugins-slate": "^5.4.4",
    "@react-page/plugins-spacer": "^5.4.4",
    "@react-page/plugins-video": "^5.4.4",
    "@react-page/react-admin": "^5.4.4"
  },
  "devDependencies": {
    "@mui/material": "^5.15.15",
    "@types/eslint": "^8.56.9",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "eslint": "^8.57.0",
    "react": "^18.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vitest": "^1.5.3"
  },
  "peerDependencies": {
    "@mui/material": "^5",
    "debug": "*",
    "react": "^18",
    "react-admin": "^4",
    "react-dom": "^18"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "src/**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  }
}
