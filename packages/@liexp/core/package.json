{
  "name": "@liexp/core",
  "version": "0.1.0",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./lib/*": "./lib/*"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc -b tsconfig.build.json",
    "watch": "tsc -b -w",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\""
  },
  "dependencies": {
    "fp-ts": "^2.16.2",
    "io-ts": "^2.2.21",
    "io-ts-types": "^0.5.19",
    "vite-plugin-css-injected-by-js": "^3.4.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-image": "^3.0.3",
    "@types/debug": "^4.1.12",
    "@types/eslint": "^8.56.2",
    "@types/express": "^4.17.21",
    "@types/mime": "^1.3.5",
    "@types/react": "^18.2.57",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "express": "^4.18.2",
    "prettier": "^3.2.5",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-html-purgecss": "^0.1.1",
    "vite-plugin-optimizer": "^1.4.3"
  },
  "peerDependencies": {
    "debug": "^4.3.1",
    "query-string": "^7.0.0",
    "uuid": "^9.0.1"
  },
  "lint-staged": {
    "**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  }
}
