{
  "name": "lies.exposed",
  "version": "0.1.0",
  "private": true,
  "description": "lies.exposed project monorepo",
  "repository": "git@github.com:lies.exposed/lies-exposed.git",
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "scripts": {
    "admin-web": "pnpm --filter 'admin-web'",
    "ai-bot": "pnpm --filter 'ai-bot'",
    "api": "pnpm --filter 'api'",
    "backend": "pnpm --filter '@liexp/backend'",
    "build": "pnpm -r run build",
    "clean": "pnpm -r run clean",
    "core": "pnpm --filter '@liexp/core'",
    "data": "pnpm --filter 'data'",
    "deploy:setup": "./deploy/setup.sh",
    "format": "pnpm -r run format",
    "lint": "pnpm -r run lint",
    "packages:build": "pnpm --filter '@liexp/*' -r run build",
    "corepack:up": "pnpm -r -c exec 'corepack up'",
    "post-deploy": "./deploy/post-deploy.sh",
    "shared": "pnpm --filter '@liexp/shared'",
    "storybook": "pnpm --filter 'storybook'",
    "test": "pnpm --filter '@liexp/test'",
    "ui": "pnpm --filter '@liexp/ui'",
    "web": "pnpm --filter 'web'",
    "vitest": "vitest",
    "vitest:expected-cov": "vitest run --coverage.enabled --coverage.thresholds.lines=90 --coverage.thresholds.functions=90 --coverage.thresholds.statements=90",
    "vitest:cov": "vitest run --coverage.enabled"
  },
  "resolutions": {
    "dotenv": "^16.4.5",
    "minimist": "^1.2.8",
    "react-dnd": "^15",
    "react-dnd-html5-backend": "^15",
    "react-is": "^18.3.1",
    "wink-nlp": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitest/coverage-v8": "^2.1.5",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-fp-ts": "^0.3.2",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^17.14.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.0",
    "husky": "^9.1.6",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.10",
    "pm2": "^5.4.3",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.15.0",
    "vitest": "^2.1.5"
  },
  "packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387",
  "engines": {
    "node": ">= 20",
    "npm": "please use pnpm >= 9",
    "pnpm": ">= 9",
    "yarn": "please use pnpm >= 9"
  }
}
