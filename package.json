{
  "name": "lies.exposed",
  "version": "0.1.0",
  "private": true,
  "description": "lies.exposed project monorepo",
  "repository": "git@github.com:lies.exposed/lies-exposed.git",
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "scripts": {
    "admin-web": "pnpm --filter 'admin-web'",
    "ai-bot": "pnpm --filter 'ai-bot'",
    "api": "pnpm --filter 'api'",
    "backend": "pnpm --filter '@liexp/backend'",
    "typecheck": "pnpm -r run typecheck",
    "build": "pnpm -r run build",
    "clean": "pnpm -r run clean",
    "core": "pnpm --filter '@liexp/core'",
    "data": "pnpm --filter 'data'",
    "worker": "pnpm --filter 'worker'",
    "deploy:setup": "./deploy/setup.sh",
    "format": "pnpm -r run format",
    "lint": "pnpm -r run lint",
    "lint:all": "eslint -c eslint.config.js",
    "corepack": "corepack",
    "corepack:up": "pnpm -r -c exec 'corepack use pnpm@latest-10'",
    "packages:build": "pnpm --filter '@liexp/*' -r run build",
    "services:clean": "pnpm -r --filter worker --filter ai-bot --filter api --filter web --filter admin-web run clean",
    "post-deploy": "./deploy/post-deploy.sh",
    "shared": "pnpm --filter '@liexp/shared'",
    "storybook": "pnpm --filter 'storybook'",
    "test": "pnpm --filter '@liexp/test'",
    "ui": "pnpm --filter '@liexp/ui'",
    "web": "pnpm --filter 'web'",
    "vitest": "vitest",
    "vitest:expected-cov": "vitest run --coverage.enabled --coverage.thresholds.lines=90 --coverage.thresholds.functions=90 --coverage.thresholds.statements=90",
    "vitest:cov": "vitest run --coverage.enabled",
    "knip": "VITE_VALIDATE_ENV=false knip"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.3",
    "corepack": "^0.34.0",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "knip": "^5.56.0",
    "lint-staged": "^16.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.3"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
  "engines": {
    "node": ">= 24",
    "npm": "please use pnpm >= 9",
    "pnpm": ">= 9",
    "yarn": "please use pnpm >= 9"
  },
  "resolutions": {
    "dotenv": "^16.4.7",
    "minimist": "^1.2.8",
    "react-dnd": "^15",
    "react-dnd-html5-backend": "^15",
    "react-is": "^18.3.1",
    "wink-nlp": "^2.3.0"
  },
  "pnpm": {
    "overrides": {
      "@ts-endpoint/core": "file:patches/ts-endpoint-core-0.1.0.tgz",
      "@ts-endpoint/http-client": "file:patches/ts-endpoint-http-client-0.1.0.tgz",
      "@ts-endpoint/resource-client": "file:patches/ts-endpoint-resource-client-0.1.0.tgz",
      "@ts-endpoint/express": "file:patches/ts-endpoint-express-0.1.0.tgz",
      "@ts-endpoint/react-admin": "file:patches/ts-endpoint-react-admin-0.1.0.tgz",
      "@ts-endpoint/tanstack-query": "file:patches/ts-endpoint-tanstack-query-0.1.0.tgz"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "exifreader",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "cpu-features",
      "protobufjs",
      "ssh2",
      "unrs-resolver"
    ]
  }
}
