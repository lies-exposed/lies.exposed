{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "includeEntryExports": true,
  "workspaces": {
    ".": {
      "ignore": [
        "services/*/build/**",
        "packages/@liexp/*/lib/**",
        "**/temp/**",
        "localai/**"
      ],
      "ignoreDependencies": [
        "husky",
        "lint-staged",
        "corepack"
      ]
    },
    "packages/@liexp/shared": {
      "includeEntryExports": false,
      "entry": [
        "src/**/*.ts"
      ],
      "project": [
        "src/**/*.ts"
      ],
      "ignoreDependencies": [
        "@floating-ui/dom"
      ]
    },
    "packages/@liexp/*": {
      "includeEntryExports": false,
      "entry": [
        "src/**/*.{ts,tsx}"
      ],
      "project": [
        "src/**/*.{ts,tsx}"
      ]
    },
    "services/web": {
      "entry": [
        "src/client/index.tsx",
        "src/server/entry.tsx",
        "test/WebAppTest.ts"
      ],
      "includeEntryExports": false,
      "project": [
        "src/**/*.{ts,tsx}",
        "test/**/*.ts"
      ],
      "ignore": [
        "src/client/templates/PageTemplate.tsx"
      ]
    },
    "services/admin": {
      "entry": [
        "src/index.tsx",
        "test/AdminAppTest.ts"
      ],
      "includeEntryExports": false,
      "project": [
        "src/**/*.{ts,tsx}"
      ],
      "ignoreDependencies": [
        "@ts-endpoint/resource-client"
      ],
      "ignore": [
        "src/hooks/useAgentAPI.ts",
        "src/hooks/useAPIAgent.ts",
        "src/hooks/useAgentDataProvider.ts"
      ]
    },
    "services/ai-bot": {
      "entry": [
        "src/run.ts"
      ],
      "project": [
        "src/**/*.ts"
      ],
      "ignoreDependencies": [
        "@aws-sdk/client-sso-oidc",
        "@aws-sdk/client-sts",
        "@esbuild/aix-ppc64"
      ]
    },
    "services/api": {
      "entry": [
        "src/run.ts",
        "src/**/*.{spec,e2e}.ts",
        "test/AppTest.ts"
      ],
      "project": [
        "src/**/*.{ts,js}",
        "test/**/*.{ts,js}",
        "!src/migrations/*.ts",
        "!src/**/*.{spec,e2e}.ts"
      ],
      "ignore": [
        "ormconfig.js",
        "test/__mocks__/**/*",
        "test/globalSetup.ts"
      ]
    },
    "services/worker": {
      "entry": [
        "src/run.ts",
        "src/bin/cli.ts",
        "test/**/*.{spec,e2e}.ts",
        "test/WorkerTest.ts"
      ],
      "project": [
        "src/**/*.ts",
        "test/**/*.{ts,js}"
      ]
    },
    "services/agent": {
      "entry": [
        "src/run.ts",
        "**/*.{spec,e2e}.ts",
        "test/AppTest.ts"
      ],
      "project": [
        "src/**/*.ts",
        "test/**/*.ts"
      ]
    },
    "services/storybook": {
      "entry": [
        ".storybook/*.{ts,tsx}",
        "src/stories/**/*.tsx"
      ],
      "includeEntryExports": false,
      "project": [
        "src/**/*.{ts,tsx}"
      ]
    }
  }
}