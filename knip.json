{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "includeEntryExports": true,
  "ignoreWorkspaces": ["services/data"],
  "workspaces": {
    ".": {
      "ignore": ["services/*/build/**", "packages/@liexp/*/lib/**"],
      "ignoreBinaries": ["corepack use pnpm@latest-10"],
      "ignoreDependencies": ["husky", "lint-staged", "corepack"],
      "ignoreUnresolved": ["tslib"]
    },
    "packages/@liexp/*": {
      "includeEntryExports": false,
      "entry": ["src/**/*.{ts,tsx}"],
      "project": ["src/**/*.{ts,tsx}"]
    },
    "services/web": {
      "entry": [
        "src/client/index.tsx",
        "src/server/server.tsx",
        "src/server/entry.tsx"
      ],
      "includeEntryExports": false,
      "project": ["src/**/*.{ts,tsx}"],
      "ignore": [
        "src/client/components/events/AddEventModal.tsx",
        "src/client/pages/DocsPage.tsx",
        "src/client/templates/PageTemplate.tsx",
        "src/client/state/commands.ts"
      ]
    },
    "services/admin-web": {
      "entry": ["src/index.tsx"],
      "includeEntryExports": false,
      "project": ["src/**/*.{ts,tsx}"]
    },
    "services/ai-bot": {
      "entry": ["src/run.ts"],
      "project": ["src/**/*.ts"],
      "ignoreDependencies": [
        "@aws-sdk/client-sso-oidc",
        "@aws-sdk/client-sts",
        "@esbuild/aix-ppc64"
      ]
    },
    "services/api": {
      "entry": [
        "src/run.ts",
        "src/**/*.{spec,e2e}.ts",
        "test/AppTest.ts",
        "test/testSetup.ts",
        "test/globalSetup.ts"
      ],
      "project": [
        "src/**/*.{ts,js}",
        "test/**/*.{ts,js}",
        "!src/migrations/*.ts",
        "!src/**/*.{spec,e2e}.ts"
      ],
      "ignore": ["ormconfig.js", "test/__mocks__/**/*"]
    },
    "services/worker": {
      "entry": ["src/run.ts", "src/bin/cli.ts"],
      "project": ["src/**/*.ts"]
    },
    "services/storybook": {
      "entry": [".storybook/*.{ts,tsx}", "src/stories/**/*.tsx"],
      "includeEntryExports": false,
      "project": ["src/**/*.{ts,tsx}"]
    }
  }
}
