{
  "name": "agent",
  "version": "0.2.5",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "imports": {
    "#cli/*": "./build/cli/*",
    "#context/*": "./build/context/*",
    "#flows/*": "./build/flows/*",
    "#io/*": "./build/io/*",
    "#providers/*": "./build/providers/*",
    "#routes/*": "./build/routes/*"
  },
  "files": [
    "build",
    "AGENTS.md"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf build",
    "cli": "node build/cli/cli.js",
    "cli:dev": "node src/cli/cli.ts",
    "dev": "tsx --watch src/run.ts",
    "docker:dev": "node --watch build/run.js",
    "lint": "eslint --cache src",
    "start": "node build/run.js",
    "test": "pnpm test:spec && pnpm test:e2e",
    "test:spec": "vitest run -c vitest.config.spec.ts",
    "test:e2e": "vitest run -c vitest.config.ts",
    "watch": "tsc -b ./tsconfig.build.json --watch",
    "watch:dev": "tsc -b --watch"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --cache --fix"
  },
  "dependencies": {
    "@langchain/anthropic": "^1.3.16",
    "@langchain/mcp-adapters": "^1.1.2",
    "@langchain/openai": "^1.2.6",
    "@langchain/xai": "^1.3.2",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/io": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@ts-endpoint/express": "file:patches/ts-endpoint-express-0.1.0.tgz",
    "axios": "^1.13.5",
    "brave-search": "^0.9.0",
    "cors": "^2.8.5",
    "debug": "^4.4.0",
    "effect": "^3.19.14",
    "express": "^5.2.1",
    "fp-ts": "^2.16.10",
    "langchain": "^1.2.19",
    "prompts": "^2.4.2",
    "puppeteer-core": "^24.8.1",
    "puppeteer-extra": "^3.3.6",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@liexp/eslint-config": "workspace:*",
    "@types/cors": "^2.8.18",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.9",
    "@types/prompts": "^2.4.9",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "supertest": "^7.1.0",
    "tsx": "^4.21.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316"
}
