{
  "name": "agent",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "imports": {
    "#cli/*": "./build/cli/*",
    "#context/*": "./build/context/*",
    "#flows/*": "./build/flows/*",
    "#io/*": "./build/io/*",
    "#providers/*": "./build/providers/*",
    "#routes/*": "./build/routes/*"
  },
  "files": [
    "build",
    "AGENTS.md"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf build",
    "cli": "node build/cli/cli.js",
    "cli:dev": "node src/cli/cli.ts",
    "dev": "tsx --watch src/run.ts",
    "docker:dev": "node --watch build/run.js",
    "lint": "eslint src",
    "start": "node build/run.js",
    "test": "echo 'No tests configured for agent'",
    "watch": "tsc -b ./tsconfig.build.json --watch",
    "watch:dev": "tsc -b --watch"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "dependencies": {
    "@langchain/mcp-adapters": "^1.1.1",
    "@langchain/openai": "^1.2.0",
    "@langchain/xai": "^1.1.0",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@ts-endpoint/express": "file:patches/ts-endpoint-express-0.1.0.tgz",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "debug": "^4.4.0",
    "effect": "^3.17.13",
    "express": "^5.2.1",
    "fp-ts": "^2.16.10",
    "langchain": "^1.2.3",
    "prompts": "^2.4.2",
    "puppeteer-core": "^24.8.1",
    "puppeteer-extra": "^3.3.6",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.1",
    "@types/node": "^24.3.0",
    "@types/prompts": "^2.4.9",
    "dotenv": "^17.2.1",
    "eslint": "^9.39.2",
    "tsx": "^4.20.6"
  },
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402"
