{
  "name": "admin-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "start": "webpack serve",
    "build": "webpack",
    "test": "jest",
    "tsc:build": "tsc -b",
    "tsc:watch": "tsc -b --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@react-page/react-admin": "^4.3.0",
    "fp-ts": "^2.9.3",
    "ol": "^6.9.0",
    "ra-core": "^3.18.0",
    "ra-i18n-polyglot": "^3.18.3",
    "ra-input-rich-text": "^3.10.1",
    "ra-ui-materialui": "^3.11.1",
    "react": "^16",
    "react-admin": "^3.11.2",
    "react-admin-color-input": "^3.0.0-beta.0",
    "react-dom": "^16",
    "react-helmet": "^6.1.0",
    "ts-jest": "^27",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-webpack-plugin": "^3.0.1",
    "typescript": "^4.1.3",
    "webpack": "^4",
    "webpack-cli": "^4.9.1"
  },
  "lint-staged": {
    "**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  },
  "_moduleAliases": {
    "@econnessione/core": "../../packages/@econnessione/core/lib",
    "@econnessione/shared": "../../packages/@econnessione/shared/lib"
  }
}
