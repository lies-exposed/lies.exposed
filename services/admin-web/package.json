{
  "name": "admin-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "files": [
    "build",
    "public"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "build:app": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "clean": "rm -rf build",
    "dev": "vite dev",
    "dev:server": "tsx watch src/server/server.tsx",
    "docker:dev": "tsx --tsconfig tsconfig.server.json -r dotenv/config --watch-path=./src ./src/server/server.tsx",
    "lint": "eslint src",
    "serve": "node build/server/server.js",
    "start": "vite start",
    "test": "vitest --passWithNoTests",
    "watch": "tsc -b --watch"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@liexp/ui": "workspace:*",
    "@tanstack/react-query": "^5.90.2",
    "@ts-endpoint/react-admin": "file:patches/ts-endpoint-react-admin-0.1.0.tgz",
    "@ts-endpoint/resource-client": "file:patches/ts-endpoint-resource-client-0.1.0.tgz",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "debug": "^4.4.0",
    "effect": "^3.17.13",
    "express": "^5.2.1",
    "fp-ts": "^2.16.10",
    "openai": "^6.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.11.0",
    "sirv": "^3.0.1",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.1",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "dotenv": "^17.2.1",
    "eslint": "^9.39.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-optimizer": "^1.4.3",
    "vite-tsconfig-paths": "^6.0.1"
  },
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402"
}
