{
  "extends": "../../.eslintrc",
  "root": true,
  "parserOptions": {
    "tsconfigRootDir": "./",
    "project": [
      "./tsconfig.json"
    ]
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "project": "./tsconfig.json"
      }
    }
  },
  "rules": {
    "@typescript-eslint/restrict-plus-operands": ["off"],
    "@typescript-eslint/no-unnecessary-type-assertion": ["off"]
  }
}
