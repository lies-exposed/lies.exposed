{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "imports": {
    "#app/*": "./build/app/*",
    "#context/*": "./build/context/*",
    "#entities/*": "./build/entities/*",
    "#flows/*": "./build/flows/*",
    "#io/*": "./build/io/*",
    "#jobs/*": "./build/jobs/*",
    "#providers/*": "./build/providers/*",
    "#queries/*": "./build/queries/*",
    "#routes/*": "./build/routes/*",
    "#services/*": "./build/services/*",
    "#types/*": "./build/types/*",
    "#utils/*": "./build/utils/*"
  },
  "files": [
    "assets",
    "build",
    "bin",
    "config"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "bin:run": "node ./build/bin/cli.js",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf build",
    "db:diagram": "node -r tsx/cjs -T ./node_modules/.bin/typeorm-uml ormconfig.js -D LR -d ./data/media/docs/diagrams/typeorm-uml.png",
    "db:dump": "tsx ./src/scripts/database.dump.ts",
    "db:dump:alpha": "DOTENV_CONFIG_PATH=.env.alpha pnpm db:dump -p alpha",
    "dev": "node --watch build/run.js",
    "dev:alpha": "DOTENV_CONFIG_PATH=.env.alpha pnpm dev",
    "docker:dev": "pnpm dev",
    "docker:worker-dev": "node --watch build/worker/index.js",
    "lint": "eslint src",
    "migration:gen": "pnpm typeorm migration:generate -d ./ormconfig.js",
    "migration:revert": "pnpm typeorm migration:revert -d ./ormconfig.js",
    "migration:run": "pnpm typeorm migration:run -d ./ormconfig.js",
    "migration:show": "pnpm typeorm migration:show -d ./ormconfig.js",
    "schema:log": "pnpm typeorm schema:log -d ./ormconfig.js",
    "start": "node build/run.js",
    "start:worker": "node build/worker/index.js",
    "test": "vitest -r ./ --workspace ./vitest.workspace.js",
    "test:all": "pnpm test:spec && pnpm test:e2e",
    "test:e2e": "vitest -c test/vitest.config.e2e.ts",
    "test:spec": "vitest -c test/vitest.config.spec.ts",
    "tsx:build": "tsx --tsconfig tsconfig.build.json",
    "typeorm": "TSX_TSCONFIG_PATH=tsconfig.build.json node --require tsx/cjs --require dotenv/config ../../node_modules/typeorm/cli.js",
    "watch": "tsc -b ./tsconfig.build.json --watch",
    "watch:dev": "tsc -b --watch"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.703.0",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "canvas": "^2.11.2",
    "cors": "2.8.5",
    "cronstrue": "^2.50.0",
    "date-fns": "^4.1.0",
    "debug": "*",
    "domino": "^2.1.6",
    "exifreader": "^4.23.5",
    "express": "^4.21.0",
    "express-jwt": "^8.4.1",
    "express-unless": "^2.1.3",
    "fluent-ffmpeg": "^2.1.2",
    "fp-ts": "^2.16.9",
    "instagram-private-api": "^1.46.1",
    "io-ts": "^2.2.21",
    "io-ts-types": "^0.5.19",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "nodemw": "^0.22.0",
    "page-metadata-parser": "^1.1.4",
    "parse2-kmz": "^1.0.7",
    "pdfjs-dist": "^4.8.69",
    "pg": "^8.11.5",
    "puppeteer-core": "^23.5.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "ts-endpoint-express": "^2.0.0",
    "tsx": "^4.19.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.2",
    "wink-eng-lite-web-model": "^1.8.0",
    "wink-nlp": "^2.3.0",
    "wink-nlp-utils": "^2.1.0"
  },
  "devDependencies": {
    "@liexp/test": "workspace:*",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.26",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.9",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/pg": "^8.11.10",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.2",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.15.0",
    "prompts": "^2.4.2",
    "supertest": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "vitest-mock-extended": "^2.0.2"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
