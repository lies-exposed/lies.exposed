    "form-data": "^4.0.0",
    "fp-ts": "^2.16.6",
    "front-matter": "4.0.2",
    "gray-matter": "^4.0.3",
    "instagram-private-api": "^1.46.1",
    "io-ts": "^2.2.21",
    "io-ts-types": "^0.5.19",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.2.13",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.55.1",
    "page-metadata-parser": "^1.1.4",
    "parse2-kmz": "^1.0.7",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.4.168",
