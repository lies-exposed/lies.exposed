{
  "name": "api",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#app/*": "./build/app/*",
    "#context/*": "./build/context/*",
    "#io/*": "./build/io/*",
    "#entities/*": "./build/entities/*",
    "#flows/*": "./build/flows/*",
    "#providers/*": "./build/providers/*",
    "#queries/*": "./build/queries/*",
    "#routes/*": "./build/routes/*",
    "#utils/*": "./build/utils/*"
  },
  "scripts": {
    "clean": "rm -rf build",
    "start": "node build/run.js",
    "develop": "tsx watch src/run.ts",
    "develop:alpha": "DOTENV_CONFIG_PATH=.env.alpha yarn develop",
    "build": "tsc -b ./tsconfig.build.json",
    "watch": "tsc -b ./tsconfig.build.json --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx bin/ src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "test": "vitest",
    "test:spec": "vitest -c test/vitest.config.spec.ts",
    "test:e2e": "vitest -c test/vitest.config.e2e.ts",
    "test:all": "yarn test:spec && yarn test:e2e",
    "tsx:build": "tsx --tsconfig tsconfig.build.json",
    "db:dump": "tsx ./src/scripts/database.dump.ts",
    "db:dump:alpha": "DOTENV_CONFIG_PATH=.env.alpha yarn db:dump -p alpha",
    "db:diagram": "node -r tsx/cjs -T ./node_modules/.bin/typeorm-uml ormconfig.js -D LR -d ./data/media/docs/diagrams/typeorm-uml.png",
    "typeorm": "TSX_TSCONFIG_PATH=tsconfig.build.json node --require tsx/cjs --require dotenv/config ../../node_modules/typeorm/cli.js",
    "schema:log": "yarn typeorm schema:log -d ./ormconfig.js",
    "migration:gen": "yarn typeorm migration:generate -d ./ormconfig.js",
    "migration:run": "yarn typeorm migration:run -d ./ormconfig.js",
    "migration:show": "yarn typeorm migration:show -d ./ormconfig.js",
    "migration:revert": "yarn typeorm migration:revert -d ./ormconfig.js",
    "create-stats": "yarn tsx:build ./bin/create-stats.ts",
    "parse-tg-message": "yarn tsx:build ./bin/parse-tg-message.ts",
    "clean-tg-messages": "yarn tsx:build ./bin/clean-tg-messages.ts",
    "upsert-tg-pinned-message": "yarn tsx:build ./bin/upsert-tg-pinned-message.ts",
    "extract-events": "yarn tsx:build ./bin/extract-events.ts",
    "upsert-nlp-entities": "yarn tsx:build ./bin/upsert-nlp-entities.ts",
    "share-post-message": "yarn tsx:build ./bin/share-post-message.ts",
    "create-from-wikipedia": "yarn tsx:build ./bin/create-from-wikipedia.ts",
    "set-default-group-usernames": "yarn tsx:build ./bin/set-default-group-usernames.ts",
    "clean-space-media": "yarn tsx:build ./bin/clean-space-media.ts",
    "import-from-kmz": "yarn tsx:build ./bin/import-from-kmz.ts",
    "extract-entities-from-url": "yarn tsx:build ./bin/extract-entities-from-url.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/lib-storage": "^3.485.0",
    "@aws-sdk/s3-request-presigner": "^3.485.0",
    "@databases/escape-identifier": "^1.0.3",
    "@liexp/backend": "0.1.0",
    "@liexp/core": "0.1.0",
    "@liexp/shared": "0.1.0",
    "@react-page/plugins-background": "^5.4.4",
    "@react-page/plugins-divider": "^5.4.4",
    "@react-page/plugins-html5-video": "^5.4.4",
    "@react-page/plugins-image": "^5.4.4",
    "@react-page/plugins-slate": "^5.4.4",
    "@react-page/plugins-spacer": "^5.4.4",
    "@react-page/plugins-video": "^5.4.4",
    "axios": "^1.6.5",
    "canvas": "^2.11.2",
    "cors": "2.8.5",
    "debug": "^4.3.4",
    "domino": "^2.1.6",
    "exifreader": "^4.20.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "fast-csv": "^4.3.6",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "fp-ts": "^2.16.2",
    "front-matter": "4.0.2",
    "gray-matter": "^4.0.3",
    "instagram-private-api": "^1.45.3",
    "io-ts": "^2.2.21",
    "io-ts-types": "^0.5.19",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.64.0",
    "nodemon": "^3.0.3",
    "page-metadata-parser": "^1.1.4",
    "parse2-kmz": "^1.0.7",
    "pdfjs-dist": "^4.0.379",
    "pg": "^8.11.3",
    "puppeteer-core": "^21.7.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.33.2",
    "ts-endpoint-express": "^2.0.0",
    "ts-shared": "2.0.0-alpha.6",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "wikipedia": "^2.1.2",
    "wink-eng-lite-web-model": "^1.5.2",
    "wink-nlp": "^1.14.3",
    "wink-nlp-utils": "^2.1.0"
  },
  "devDependencies": {
    "@liexp/test": "0.1.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/eslint": "^8.56.2",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.19.10",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.2",
    "@types/pg": "^8.11.0",
    "@types/prettier": "^3.0.0",
    "@types/prompts": "^2.4.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.4",
    "prompts": "^2.4.2",
    "supertest": "^6.3.4",
    "typeorm-uml": "^1.6.5",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "license": "MIT",
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "src/**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  }
}
