{
  "name": "api",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "start": "node build/run.js",
    "develop": "DOTENV_CONFIG_PATH=../../.env nodemon -r dotenv/config --delay 80ms build/run.js",
    "develop:alpha": "DOTENV_CONFIG_PATH=../../.env.alpha nodemon -r dotenv/config build/run.js",
    "build": "tsc -b ./tsconfig.build.json",
    "watch": "tsc -b ./tsconfig.build.json --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "test:e2e": "jest -c jest.config.js e2e",
    "ts:node": "ts-node -r dotenv/config -r tsconfig-paths/register",
    "create-pages": "yarn ts:node ./scripts/create-pages.ts",
    "fixtures:load": "yarn ts:node ./src/scripts/fixtures/load-fixtures.ts",
    "db:dump": "yarn ts:node ./src/scripts/database.dump.ts",
    "db:dump:alpha": "DOTENV_CONFIG_PATH=../../.env.alpha yarn db:dump -p alpha",
    "db:diagram": "ts-node -r tsconfig-paths/register -T ./node_modules/.bin/typeorm-uml ormconfig.js -D LR -d ./data/media/docs/diagrams/typeorm-uml.png",
    "migration:gen": "typeorm migration:generate --check",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert"
  },
  "dependencies": {
    "@databases/escape-identifier": "^1.0.2",
    "@databases/pg": "^5.3.0",
    "aws-sdk": "^2.1064.0",
    "axios": "^0.25.0",
    "cors": "2.8.5",
    "debug": "^4.3.3",
    "domino": "^2.1.6",
    "express": "4.17.2",
    "express-jwt": "^6.1.0",
    "fast-csv": "^4.3.6",
    "form-data": "^4.0.0",
    "fp-ts": "^2.11.8",
    "front-matter": "4.0.2",
    "gray-matter": "^4.0.3",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.4",
    "node-cron": "^3.0.0",
    "page-metadata-parser": "^1.1.4",
    "pg": "^8.7.1",
    "puppeteer-core": "^10.4.0",
    "ts-endpoint-express": "^2.0.0",
    "ts-jest": "^27.1.3",
    "ts-shared": "2.0.0-alpha.6",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@dicebear/avatars": "^4.10.2",
    "@dicebear/avatars-avataaars-sprites": "^4.10.2",
    "@dicebear/avatars-bottts-sprites": "^4.10.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.4",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.9",
    "@types/node-cron": "^2.0.5",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "fast-check": "^2.21.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.4.0",
    "typeorm-uml": "^1.6.4",
    "typescript": "^4.5.5"
  },
  "_moduleAliases": {
    "@io": "build/io",
    "@entities": "build/entities",
    "@providers": "build/providers",
    "@routes": "build/routes",
    "@utils": "build/utils",
    "@econnessione/core": "../../packages/@econnessione/core/lib",
    "@econnessione/shared": "../../packages/@econnessione/shared/lib"
  },
  "license": "MIT",
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "src/**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  }
}
