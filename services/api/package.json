{
  "name": "api",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "start": "node build/run.js",
    "develop": "DOTENV_CONFIG_PATH=../../.env nodemon -r dotenv/config --delay 80ms build/run.js",
    "develop:alpha": "DOTENV_CONFIG_PATH=../../.env.alpha nodemon -r dotenv/config build/run.js",
    "build": "tsc -b ./tsconfig.build.json",
    "watch": "tsc -b ./tsconfig.build.json --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "test": "jest",
    "test:e2e": "jest e2e",
    "test:spec": "jest spec",
    "ts:node": "ts-node -r dotenv/config -r tsconfig-paths/register",
    "create-pages": "yarn ts:node ./scripts/create-pages.ts",
    "fixtures:load": "yarn ts:node ./src/scripts/fixtures/load-fixtures.ts",
    "db:dump": "yarn ts:node ./src/scripts/database.dump.ts",
    "db:dump:alpha": "DOTENV_CONFIG_PATH=../../.env.alpha yarn db:dump -p alpha",
    "db:diagram": "ts-node -r tsconfig-paths/register -T ./node_modules/.bin/typeorm-uml ormconfig.js -D LR -d ./data/media/docs/diagrams/typeorm-uml.png",
    "migration:gen": "typeorm migration:generate -d ./ormconfig.js --check",
    "migration:run": "typeorm migration:run -d ./ormconfig.js",
    "migration:revert": "typeorm migration:revert -d ./ormconfig.js",
    "create-event-stats": "DOTENV_CONFIG_PATH=../../.env yarn ts:node ./bin/create-event-stats.ts",
    "parse-tg-message": "DOTENV_CONFIG_PATH=../../.env yarn ts:node ./bin/parse-tg-message.ts",
    "extract-events": "DOTENV_CONFIG_PATH=../../.env yarn ts:node ./bin/extract-events.ts"
  },
  "dependencies": {
    "@databases/escape-identifier": "^1.0.3",
    "@databases/pg": "^5.4.1",
    "aws-sdk": "^2.1226.0",
    "axios": "^0.27.2",
    "canvas": "^2.10.1",
    "cors": "2.8.5",
    "debug": "^4.3.4",
    "domino": "^2.1.6",
    "express": "^4.18.2",
    "express-jwt": "^6.1.2",
    "fast-csv": "^4.3.6",
    "form-data": "^4.0.0",
    "fp-ts": "^2.13.1",
    "front-matter": "4.0.2",
    "gray-matter": "^4.0.3",
    "io-ts": "^2.2.19",
    "io-ts-types": "^0.5.19",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "node-telegram-bot-api": "^0.59.0",
    "page-metadata-parser": "^1.1.4",
    "pdfjs-dist": "^2.16.105",
    "pg": "^8.8.0",
    "puppeteer-core": "^13.7.0",
    "ts-endpoint-express": "^2.0.0",
    "ts-jest": "^28.0.8",
    "ts-shared": "2.0.0-alpha.6",
    "typeorm": "^0.3.10",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@dicebear/avatars": "^4.10.5",
    "@dicebear/avatars-avataaars-sprites": "^4.10.5",
    "@dicebear/avatars-bottts-sprites": "^4.10.5",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.14",
    "@types/express-jwt": "^6.0.4",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.66",
    "@types/node-cron": "^3.0.4",
    "@types/node-telegram-bot-api": "^0.57.5",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "typeorm-uml": "^1.6.5",
    "typescript": "^4.8.4"
  },
  "_moduleAliases": {
    "@io": "build/io",
    "@entities": "build/entities",
    "@flows": "build/flows",
    "@routes": "build/routes",
    "@utils": "build/utils",
    "@liexp/core": "../../packages/@liexp/core/lib",
    "@liexp/shared": "../../packages/@liexp/shared/lib",
    "@liexp/ui": "../../packages/@liexp/ui/lib"
  },
  "license": "MIT",
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "src/**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  }
}
