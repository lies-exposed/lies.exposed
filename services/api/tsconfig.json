{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "./build",
    "baseUrl": "./src",
    "jsx": "react-jsx",
    "rootDir": "./",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "tsBuildInfoFile": "build/.tsbuildinfo",
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "allowJs": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": false,
    "paths": {
      "#app/*": [
        "./app/*"
      ],
      "#context/*": [
        "./context/*"
      ],
      "#flows/*": [
        "./flows/*"
      ],
      "#io/*": [
        "./io/*"
      ],
      "#jobs/*": [
        "./jobs/*"
      ],
      "#providers/*": [
        "providers/*"
      ],
      "#queries/*": [
        "queries/*"
      ],
      "#routes/*": [
        "./routes/*"
      ],
      "#services/*": [
        "./services/*"
      ],
      "#types/*": [
        "./types/*"
      ],
      "#utils/*": [
        "./utils/*"
      ]
    }
  },
  "include": [
    "./src",
    "./scripts",
    "./test",
    "../../packages/@liexp/backend/typings/**/*.d.ts",
    "../../packages/@liexp/shared/typings/**/*.d.ts"
  ],
  "exclude": [
    "**/build",
    "**/lib"
  ],
  "references": [
    {
      "path": "../../packages/@liexp/core"
    },
    {
      "path": "../../packages/@liexp/shared"
    },
    {
      "path": "../../packages/@liexp/backend"
    }
  ]
}