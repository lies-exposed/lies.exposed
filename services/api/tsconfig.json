{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "./build",
    "baseUrl": "./src",
    "jsx": "react",
    "rootDir": "./",
    "lib": ["DOM", "ES2022"],
    "tsBuildInfoFile": "build/.tsbuildinfo",
    "typeRoots": [
      "./typings",
      "../../node_modules/@types",
      "../../packages/@liexp/backend/typings",
      "../../packages/@liexp/shared/typings"
    ],
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowJs": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "#app/*": ["app/*"],
      "#context/*": ["context/*"],
      "#io/*": ["io/*"],
      "#entities/*": ["entities/*"],
      "#flows/*": ["flows/*"],
      "#providers/*": ["providers/*"],
      "#queries/*": ["queries/*"],
      "#routes/*": ["routes/*"],
      "#utils/*": ["utils/*"]
    }
  },
  "include": [
    "../../node_modules/vitest/globals.d.ts",
    "./bin",
    "./src",
    "./scripts",
    "./test",
    ".eslintrc.cjs",
    "vitest.workspace.ts"
  ],
  "exclude": ["**/build", "**/lib"]
}
