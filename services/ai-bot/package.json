{
  "name": "ai-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "main": "build/run.js",
  "imports": {
    "#common/*": "./build/common/*",
    "#flows/*": "./build/flows/*",
    "#services/*": "./build/services/*"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rm -rf build/.build.* && rm -rf build/*",
    "typecheck": "tsc --noEmit",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build:es": "esbuild --format=cjs --target=node20 --platform=node --bundle --outfile=build/run-esbuild.js build/run.js",
    "build": "pnpm build:tsc && pnpm build:es",
    "dev": "node --watch build/run.js",
    "dev:alpha": "DOTENV_CONFIG_PATH=.env.alpha pnpm dev",
    "docker:dev": "pnpm dev",
    "cli:dev": "node --import tsx/esm src/cli/index.ts",
    "lint": "eslint src",
    "start": "node build/run.js",
    "pkg": "./scripts/pkg.sh",
    "postinstall": "cp -r ../../node_modules/pdfjs-dist/build/pdf.worker.mjs ./build/pdf.worker.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-sso-oidc": "^3.797.0",
    "@aws-sdk/client-sts": "^3.797.0",
    "@langchain/community": "^0.3.35",
    "@langchain/core": "^0.3.49",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@ts-endpoint/core": "file:patches/ts-endpoint-core-0.1.0.tgz",
    "@ts-endpoint/resource-client": "file:patches/ts-endpoint-resource-client-0.1.0.tgz",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "debug": "^4.4.0",
    "effect": "^3.14.16",
    "fp-ts": "^2.16.10",
    "langchain": "^0.3.24",
    "pdfjs-dist": "^4.10.38",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@esbuild/aix-ppc64": "^0.25.3",
    "@types/node": "^22.15.3",
    "@types/prompts": "^2.4.9",
    "esbuild": "^0.25.2",
    "eslint": "^9.25.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677"
}
