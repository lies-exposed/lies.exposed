{
  "name": "ai-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "main": "build/run-esbuild.cjs",
  "type": "module",
  "imports": {
    "#common/*": "./build/common/*",
    "#flows/*": "./build/flows/*",
    "#services/*": "./build/services/*"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rm -rf build/.build.* && rm -rf build/*",
    "typecheck": "tsc --noEmit",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build:es": "node --import tsx/esm ./esbuild.config.mjs",
    "build": "pnpm build:tsc && pnpm build:es",
    "dev": "node --watch ./build/run-esbuild.cjs --trace-warnings",
    "docker:dev": "pnpm dev",
    "cli:dev": "node --import tsx/esm src/cli/index.ts",
    "lint": "eslint src",
    "start": "node build/run.js",
    "pkg": "./scripts/pkg.sh"
  },
  "dependencies": {
    "@aws-sdk/client-sso-oidc": "^3.913.0",
    "@aws-sdk/client-sts": "^3.913.0",
    "@langchain/community": "^1.0.0",
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "^1.0.0",
    "@langchain/mcp-adapters": "^1.0.0",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@napi-rs/canvas": "^0.1.80",
    "@ts-endpoint/core": "file:patches/ts-endpoint-core-0.1.0.tgz",
    "@ts-endpoint/resource-client": "file:patches/ts-endpoint-resource-client-0.1.0.tgz",
    "axios": "^1.12.2",
    "debug": "^4.4.0",
    "effect": "^3.17.13",
    "fp-ts": "^2.16.10",
    "langchain": "^1.0.1",
    "openai": "^5.23.2",
    "pdfjs-dist": "^5.4.149",
    "prompts": "^2.4.2",
    "puppeteer-core": "^24.8.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@esbuild/aix-ppc64": "^0.25.4",
    "@types/node": "^24.3.0",
    "@types/prompts": "^2.4.9",
    "esbuild": "^0.25.9",
    "eslint": "^9.35.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
