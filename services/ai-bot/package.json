{
  "name": "ai-bot",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#common/*": "./build/common/*",
    "#flows/*": "./build/flows/*",
    "#services/*": "./build/services/*"
  },
  "scripts": {
    "clean": "rm -rf build",
    "typecheck": "tsc --noEmit",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build:es": "esbuild --format=cjs --target=node20 --platform=node --bundle --outfile=build/run-esbuild.js build/run.js",
    "build": "pnpm build:tsc && pnpm build:es",
    "dev": "node --watch build/run.js",
    "dev:alpha": "DOTENV_CONFIG_PATH=.env.alpha pnpm dev",
    "docker:dev": "pnpm dev",
    "lint": "eslint src",
    "start": "node build/run.js",
    "pkg": "./scripts/pkg.sh",
    "postinstall": "cp -r ../../node_modules/pdfjs-dist/build/pdf.worker.mjs ./build/pdf.worker.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-sso-oidc": "^3.758.0",
    "@aws-sdk/client-sts": "^3.758.0",
    "@langchain/community": "^0.3.35",
    "@langchain/core": "^0.3.42",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@liexp/test": "workspace:*",
    "@ts-endpoint/core": "^0.1.0",
    "@ts-endpoint/resource-client": "^0.1.0",
    "axios": "^1.8.0",
    "cheerio": "^1.0.0",
    "effect": "^3.14.8",
    "fp-ts": "^2.16.9",
    "langchain": "^0.3.19",
    "pdfjs-dist": "^4.10.38",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@esbuild/aix-ppc64": "^0.25.2",
    "@types/node": "^22.13.10",
    "@types/prompts": "^2.4.9",
    "esbuild": "^0.25.2",
    "eslint": "^9.21.0",
    "typescript": "^5.8.2"
  }
}
