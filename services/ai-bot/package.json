{
  "name": "ai-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "main": "build/run-esbuild.js",
  "type": "module",
  "imports": {
    "#common/*": "./build/common/*",
    "#flows/*": "./build/flows/*",
    "#services/*": "./build/services/*"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rm -rf build/.build.* && rm -rf build/*",
    "typecheck": "tsc --noEmit",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build:es": "node --import tsx/esm ./esbuild.config.mjs",
    "build": "pnpm build:tsc && pnpm build:es",
    "dev": "node --watch build/run.js --trace-warnings",
    "dev:alpha": "DOTENV_CONFIG_PATH=.env.alpha pnpm dev",
    "docker:dev": "pnpm dev",
    "cli:dev": "node --import tsx/esm src/cli/index.ts",
    "lint": "eslint src",
    "start": "node build/run.js",
    "pkg": "./scripts/pkg.sh"
  },
  "dependencies": {
    "@aws-sdk/client-sso-oidc": "^3.806.0",
    "@aws-sdk/client-sts": "^3.806.0",
    "@langchain/community": "^0.3.42",
    "@langchain/core": "^0.3.55",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@napi-rs/canvas": "^0.1.70",
    "@ts-endpoint/core": "file:patches/ts-endpoint-core-0.1.0.tgz",
    "@ts-endpoint/resource-client": "file:patches/ts-endpoint-resource-client-0.1.0.tgz",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "debug": "^4.4.0",
    "effect": "^3.14.18",
    "fp-ts": "^2.16.10",
    "langchain": "^0.3.24",
    "pdfjs-dist": "^5.2.133",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@esbuild/aix-ppc64": "^0.25.4",
    "@types/node": "^22.15.17",
    "@types/prompts": "^2.4.9",
    "esbuild": "^0.25.2",
    "eslint": "^9.25.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677"
}
