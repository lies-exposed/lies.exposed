{
  "name": "ai-bot",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#common/*": "./build/common/*",
    "#flows/*": "./build/flows/*",
    "#services/*": "./build/services/*"
  },
  "scripts": {
    "clean": "rm -rf build",
    "typecheck": "tsc --noEmit",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build:es": "esbuild --format=cjs --target=node20 --platform=node --bundle --outfile=build/run-esbuild.js build/run.js",
    "build": "pnpm build:tsc && pnpm build:es",
    "dev": "node --watch build/run.js",
    "dev:alpha": "DOTENV_CONFIG_PATH=.env.alpha pnpm dev",
    "docker:dev": "pnpm dev",
    "lint": "eslint src",
    "start": "node build/run.js",
    "pkg": "./scripts/pkg.sh"
  },
  "dependencies": {
    "@aws-sdk/client-sso-oidc": "^3.731.1",
    "@aws-sdk/client-sts": "^3.731.1",
    "@langchain/community": "^0.3.27",
    "@langchain/core": "^0.3.36",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "fp-ts": "^2.16.9",
    "io-ts": "^2.2.22",
    "langchain": "^0.3.13",
    "pdfjs-dist": "^4.10.38",
    "prompts": "^2.4.2",
    "ts-shared": "^2.0.0-alpha.6"
  },
  "devDependencies": {
    "eslint": "^9.18.0",
    "typescript": "^5.7.3"
  }
}
