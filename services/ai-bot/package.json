{
  "name": "ai-bot",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#common/*": "./build/common/*",
    "#flows/*": "./build/flows/*",
    "#services/*": "./build/services/*"
  },
  "scripts": {
    "clean": "rm -rf build",
    "typecheck": "tsc --noEmit",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build:es": "esbuild --format=cjs --target=node20 --platform=node --bundle --outfile=build/run-esbuild.js build/run.js",
    "build": "pnpm build:tsc && pnpm build:es",
    "dev": "node --watch build/run.js",
    "dev:alpha": "DOTENV_CONFIG_PATH=.env.alpha pnpm dev",
    "docker:dev": "pnpm dev",
    "lint": "eslint src",
    "start": "node build/run.js",
    "pkg": "./scripts/pkg.sh"
  },
  "dependencies": {
    "@aws-sdk/client-sso-oidc": "catalog:",
    "@aws-sdk/client-sts": "catalog:",
    "@langchain/community": "catalog:",
    "@langchain/core": "catalog:",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@liexp/test": "workspace:*",
    "axios": "catalog:",
    "cheerio": "catalog:",
    "fp-ts": "catalog:",
    "io-ts": "catalog:",
    "langchain": "catalog:",
    "pdfjs-dist": "catalog:",
    "prompts": "catalog:",
    "ts-shared": "catalog:"
  },
  "devDependencies": {
    "eslint": "catalog:",
    "typescript": "catalog:"
  }
}
