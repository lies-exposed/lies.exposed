{
  "name": "ai-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "main": "build/run-esbuild.js",
  "type": "module",
  "imports": {
    "#common/*": "./build/common/*",
    "#flows/*": "./build/flows/*",
    "#services/*": "./build/services/*"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rm -rf build/.build.* && rm -rf build/*",
    "typecheck": "tsc --noEmit",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build:es": "node --import tsx/esm ./esbuild.config.mjs",
    "build": "pnpm build:tsc && pnpm build:es",
    "dev": "node --watch build/run.js --trace-warnings",
    "dev:alpha": "DOTENV_CONFIG_PATH=.env.alpha pnpm dev",
    "docker:dev": "pnpm dev",
    "cli:dev": "node --import tsx/esm src/cli/index.ts",
    "lint": "eslint src",
    "start": "node build/run.js",
    "pkg": "./scripts/pkg.sh"
  },
  "dependencies": {
    "@aws-sdk/client-sso-oidc": "^3.812.0",
    "@aws-sdk/client-sts": "^3.812.0",
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.61",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@napi-rs/canvas": "^0.1.70",
    "@ts-endpoint/core": "file:patches/ts-endpoint-core-0.1.0.tgz",
    "@ts-endpoint/resource-client": "file:patches/ts-endpoint-resource-client-0.1.0.tgz",
    "axios": "^1.9.0",
    "debug": "^4.4.0",
    "effect": "^3.14.18",
    "fp-ts": "^2.16.10",
    "langchain": "^0.3.29",
    "pdfjs-dist": "^5.2.133",
    "prompts": "^2.4.2",
    "puppeteer-core": "^24.8.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@esbuild/aix-ppc64": "^0.25.4",
    "@types/node": "^22.15.17",
    "@types/prompts": "^2.4.9",
    "esbuild": "^0.25.2",
    "eslint": "^9.28.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
