{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./build",
    "baseUrl": "./src",
    "rootDir": "./src",
    "lib": ["ESNext", "DOM"],
    "tsBuildInfoFile": "build/.tsbuildinfo",
    "typeRoots": [
      "./src/typings",
      "node_modules/@types",
      "../../node_modules/@types"
    ],
    "declaration": false,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "paths": {
      "@liexp/core/*": ["../../../packages/@liexp/core/src/*"],
      "@liexp/shared/*": [
        "../../../packages/@liexp/shared/src/*"
      ],
      "@liexp/ui/*": ["../../../packages/@liexp/ui/src/*"]
    }
  },
  "references": [
    {
      "path": "../../packages/@liexp/core"
    },
    {
      "path": "../../packages/@liexp/shared"
    },
    {
      "path": "../../packages/@liexp/ui"
    }
  ],
  "include": ["./src"],
  "exclude": []
}
