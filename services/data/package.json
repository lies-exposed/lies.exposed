{
  "name": "data",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "test:e2e": "jest -c jest.config.js e2e",
    "serve": "DOTENV_CONFIG_PATH=../../.env.local ts-node -r dotenv/config -r tsconfig-paths/register ./src/serve",
    "vaccines:download": "DOTENV_CONFIG_PATH=../../.env.local ts-node -r dotenv/config -r tsconfig-paths/register ./src/vaccines/download",
    "vaccines:parse": "DOTENV_CONFIG_PATH=../../.env.local ts-node -r dotenv/config -r tsconfig-paths/register ./src/vaccines/parse"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  }
}
