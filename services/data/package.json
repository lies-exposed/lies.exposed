{
  "name": "data",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "start": "node -r dotenv/config build/serve.js",
    "develop": "DOTENV_CONFIG_PATH=../../.env.local nodemon -r dotenv/config build/serve.js",
    "build": "tsc -b",
    "watch": "tsc -b --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "test:e2e": "vitest -c vitest.e2e.config.ts",
    "tsx:dev": "DOTENV_CONFIG_PATH=../../.env.local node -r tsx/cjs -r dotenv/config",
    "serve": "yarn tsx:dev ./src/serve",
    "vaccines:download": "yarn tsx:dev ./src/vaccines/download",
    "vaccines:parse": "yarn tsx:dev ./src/vaccines/parse",
    "bucket:pull": "aws s3 sync s3://econnessione-alpha/public ./buckets/public --endpoint https://fra1.digitaloceanspaces.com/",
    "clean:data": "rm -rf ./public/media/actors ./public/media/groups ./public/media/images"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "express": "^4.17.3",
    "fp-ts": "^2.12.0",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "@types/node": "^18",
    "dotenv": "^14.3.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "typescript": "^4.6.2"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  }
}
