{
  "name": "data",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "scripts": {
    "bucket:pull": "aws s3 sync s3://econnessione-alpha/public ./buckets/public --endpoint https://fra1.digitaloceanspaces.com/",
    "build": "tsc -b",
    "clean": "rm -rf build",
    "clean:data": "rm -rf ./public/media/actors ./public/media/groups ./public/media/images",
    "develop": "DOTENV_CONFIG_PATH=../../.env.local node --import tsx/esm -r dotenv/config src/serve.ts",
    "lint": "eslint src",
    "serve": "pnpm tsx:dev ./src/serve",
    "start": "node -r dotenv/config build/serve.js",
    "test:e2e": "vitest -c vitest.e2e.config.ts",
    "tsx:dev": "DOTENV_CONFIG_PATH=../../.env.local node -r tsx/cjs -r dotenv/config",
    "vaccines:download": "pnpm tsx:dev ./src/vaccines/download",
    "vaccines:parse": "pnpm tsx:dev ./src/vaccines/parse",
    "watch": "tsc -b --watch"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "express": "^4.17.3",
    "fp-ts": "^2.12.0",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "dotenv": "^16.4.7",
    "prettier": "^2.5.1",
    "supertest": "^7",
    "typescript": "^4.6.2"
  }
}
