VIRTUAL_HOST=admin.liexp.dev
VIRTUAL_PORT=80

# Node Environment
NODE_ENV=development
DEBUG=@liexp:*

# Admin Server Configuration (for proxy server)
# Use different port for development server (3001) vs container port (80)
SERVER_PORT=3001
SERVER_HOST=0.0.0.0

# JWT Secret (must match agent/api services)
JWT_SECRET=my-secret

# Agent Service URL
AGENT_API_URL=http://agent.liexp.dev/v1

# ServiceClient Identity (M2M authentication)
# TODO: Generate proper UUIDs for production
SERVICE_CLIENT_ID=00000000-0000-0000-0000-000000000001
SERVICE_CLIENT_USER_ID=00000000-0000-0000-0000-000000000002
SERVICE_CLIENT_PERMISSIONS=admin:read

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
