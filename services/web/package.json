{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "keywords": [],
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "files": [
    "build",
    "public"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "build:app-server": "pnpm build:client && pnpm build:server-ssr && pnpm build:server",
    "build:client": "vite build --outDir build/client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:server-ssr": "vite build --outDir build/server --ssr src/server/entry.tsx",
    "clean": "rm -rf build",
    "dev": "DOTENV_CONFIG_PATH=.env node --import tsx/esm -r dotenv/config --watch-path=./src ./src/server/server.tsx",
    "docker:dev": "DOTENV_CONFIG_PATH=.env tsx --tsconfig tsconfig.server.json -r dotenv/config --watch-path=./src ./src/server/server.tsx",
    "lint": "eslint src",
    "scripts:make-avatars-mock": "tsx ./scripts/make-avatars-mock.ts",
    "serve": "node ./build/server/server.js",
    "test": "vitest",
    "watch": "tsc -b --watch",
    "watch:client": "DOTENV_CONFIG_PATH=.env.development vite dev"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@liexp/ui": "workspace:*",
    "@tanstack/react-query": "^5.90.2",
    "@ts-endpoint/react-admin": "file:patches/ts-endpoint-react-admin-0.1.0.tgz",
    "@ts-endpoint/tanstack-query": "file:patches/ts-endpoint-tanstack-query-0.1.0.tgz",
    "compression": "^1.8.0",
    "date-fns": "^4.1.0",
    "debug": "^4.4.0",
    "effect": "^3.17.13",
    "express": "^5.1.0",
    "fp-ts": "^2.16.10",
    "ra-core": "^5.12.1",
    "ra-i18n-polyglot": "^5.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-router": "^7.9.2",
    "sirv": "^3.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.1",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "dotenv": "^17.2.1",
    "eslint": "^9.35.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^7.1.12",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-optimizer": "^1.4.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
