{
  "name": "web",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "scripts": {
    "clean": "rm -rf build",
    "dev": "DOTENV_CONFIG_PATH=.env node --import tsx/esm -r dotenv/config ./src/server/server.tsx",
    "watch:client": "DOTENV_CONFIG_PATH=.env.development vite dev",
    "build:client": "vite build --outDir build/client",
    "build:server-ssr": "vite build --outDir build/server --ssr src/server/entry.tsx",
    "build:server": "tsc -p tsconfig.server.json",
    "build:app-server": "yarn build:client && yarn build:server-ssr && yarn build:server",
    "build": "tsc -b tsconfig.build.json",
    "serve": "node ./build/server/server.js",
    "test": "vitest",
    "watch": "tsc -b --watch",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "scripts:make-avatars-mock": "tsx ./scripts/make-avatars-mock.ts"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/server": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@liexp/core": "0.1.0",
    "@liexp/react-page": "0.1.0",
    "@liexp/shared": "0.1.0",
    "@liexp/ui": "0.1.0",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@tanstack/react-query": "^5.28.4",
    "@visx/axis": "^3.10.1",
    "@visx/gradient": "^3.3.0",
    "@visx/legend": "^3.5.0",
    "@visx/network": "^3.3.0",
    "@visx/responsive": "^3.10.2",
    "@visx/tooltip": "^3.3.0",
    "axios": "^1.6.8",
    "bs58": "^5.0.0",
    "clsx": "^2.1.0",
    "compression": "^1.7.4",
    "d3": "^7.9.0",
    "debug": "^4.3.4",
    "express": "^4.19.2",
    "fp-ts": "^2.16.4",
    "io-ts": "^2.2.21",
    "io-ts-types": "^0.5.19",
    "is-plain-object": "^5.0.0",
    "module-alias": "^2.2.3",
    "ol": "^8.2.0",
    "page-metadata-parser": "^1.1.4",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.3",
    "ra-core": "^4.16.12",
    "ra-i18n-polyglot": "^4.16.12",
    "ra-input-rich-text": "^4.16.13",
    "ra-ui-materialui": "^4.16.13",
    "react": "^18.2.0",
    "react-admin": "^4.16.13",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.22.3",
    "react-share": "^5.1.0",
    "react-slick": "^0.30.2",
    "react-spring": "^9.7.3",
    "react-toc": "^3.1.0",
    "reactflow": "^11.10.4",
    "sirv": "^2.0.4",
    "slick-carousel": "^1.8.1",
    "throttle-debounce": "^5.0.0",
    "topojson-client": "3.1.0",
    "topojson-server": "3.0.1",
    "uuid": "^9.0.1",
    "world-atlas": "2.0.2"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.4",
    "@types/compression": "^1.7.5",
    "@types/d3": "^7.4.3",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/mime": "^1.3.5",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-calendar-heatmap": "^1.6.7",
    "@types/react-dom": "^18.2.22",
    "@types/react-helmet": "^6.1.11",
    "@types/react-slick": "^0.23.13",
    "@types/throttle-debounce": "^5.0.2",
    "@types/topojson-client": "^3.1.4",
    "@types/topojson-server": "^3.0.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vite": "^5.2.6"
  },
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "src/**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  }
}
