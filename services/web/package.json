{
  "name": "web",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "scripts": {
    "clean": "rm -rf build",
    "watch:app": "DOTENV_CONFIG_PATH=.env.development webpack watch",
    "watch:server": "DOTENV_CONFIG_PATH=.env.development webpack watch -c webpack.server.ts",
    "build:app": "webpack -c webpack.config.ts",
    "build:server": "webpack -c webpack.server.ts",
    "build:app-server": "yarn build:app && yarn build:server",
    "serve": "nodemon ./build/server/ssr.js",
    "test": "jest",
    "build": "tsc -b",
    "watch": "tsc -b --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "scripts:make-avatars-mock": "ts-node ./scripts/make-avatars-mock.ts",
    "bundle-stats-app": "webpack-bundle-analyzer build/stats.json",
    "bundle-stats-server": "webpack-bundle-analyzer build/server/stats.json"
  },
  "dependencies": {
    "@emotion/css": "^11.10.6",
