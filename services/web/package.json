    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/material": "^5.11.14",
    "@visx/axis": "^3.1.0",
    "@visx/gradient": "^3.0.0",
    "@visx/legend": "^3.0.0",
    "@visx/network": "^3.0.0",
    "@visx/responsive": "^3.0.0",
    "@visx/tooltip": "^3.0.0",
    "axios": "^1.3.4",
    "bs58": "^5.0.0",
    "clsx": "^1.2.1",
    "d3": "^7.8.2",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "is-plain-object": "^5.0.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.21",
