{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "keywords": [],
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "files": [
    "build",
    "public"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "build:app-server": "pnpm build:client && pnpm build:server-ssr && pnpm build:server",
    "build:client": "vite build --outDir build/client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:server-ssr": "vite build --outDir build/server --ssr src/server/entry.tsx",
    "clean": "rm -rf build",
    "dev": "DOTENV_CONFIG_PATH=.env node --import tsx/esm -r dotenv/config ./src/server/server.tsx",
    "docker:dev": "node --import tsx/esm -r dotenv/config ./src/server/server.tsx",
    "lint": "eslint src",
    "scripts:make-avatars-mock": "tsx ./scripts/make-avatars-mock.ts",
    "serve": "node ./build/server/server.js",
    "test": "vitest",
    "watch": "tsc -b --watch",
    "watch:client": "DOTENV_CONFIG_PATH=.env.development vite dev"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@liexp/ui": "workspace:*",
    "@tanstack/react-query": "^5.64.1",
    "axios": "^1.7.7",
    "bs58": "^5.0.0",
    "clsx": "^2.1.0",
    "compression": "^1.7.4",
    "date-fns": "^4.1.0",
    "debug": "^4.3.4",
    "express": "^4.21.0",
    "fp-ts": "^2.16.9",
    "io-ts": "^2.2.22",
    "io-ts-types": "^0.5.19",
    "query-string": "^7.1.3",
    "ra-core": "^5.5.2",
    "ra-i18n-polyglot": "^5.5.2",
    "react": "^19.0.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-router": "^6.28.1",
    "react-router-dom": "^6.28.1",
    "sirv": "^3.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^18.3.5",
    "dotenv": "^16.4.7",
    "eslint": "^9.19.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-optimizer": "^1.4.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
