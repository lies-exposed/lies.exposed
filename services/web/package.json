{
  "name": "web",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "scripts": {
    "clean": "rm -rf build",
    "start": "NODE_ENV=development webpack serve",
    "build": "webpack",
    "test": "jest",
    "tsc:build": "tsc -b",
    "tsc:watch": "tsc -b --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "scripts:make-avatars-mock": "ts-node ./scripts/make-avatars-mock.ts",
    "stats": "webpack-bundle-analyzer public/stats.json"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@mdi/font": "^5.0.45",
    "avenger": "^7.0.1",
    "axios": "^0.24.0",
    "bs58": "^4.0.1",
    "d3": "^5.16.0",
    "date-fns": "^2.15.0",
    "fp-ts": "^2.9.1",
    "io-ts": "^2.2.13",
    "io-ts-types": "^0.5.12",
    "node-sass": "4.14.1",
    "ol": "^6.9.0",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "react": "^16.9",
    "react-dom": "^16.9.0",
    "react-error-boundary": "^3.1.0",
    "react-graph-vis": "^1.0.7",
    "react-helmet": "^6.1.0",
    "react-quill": "1.3.5",
    "react-spring": "^9.1.2",
    "react-toc": "^2.0.0",
    "react-vertical-timeline-component": "^3.3.3",
    "slick-carousel": "^1.8.1",
    "source-serif-pro": "^3.1.0",
    "throttle-debounce": "^2.2.1",
    "topojson-client": "3.1.0",
    "topojson-server": "3.0.1",
    "uuid": "^8.3.2",
    "world-atlas": "2.0.2"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/ol": "^6.4.1",
    "@types/react": "^16.9.35",
    "@types/react-calendar-heatmap": "^1.6.2",
    "@types/react-helmet": "^6.1.0",
    "@types/react-vertical-timeline-component": "^3.0.0",
    "@types/throttle-debounce": "^2.1.0",
    "@types/topojson-client": "^3.0.0",
    "@types/topojson-server": "^3.0.0",
    "@types/uuid": "^8.0.1",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-webpack-plugin": "^3.0.1",
    "prettier": "^2.2.1",
    "react-app-rewire-alias": "^1.0.1",
    "react-app-rewired": "^2.1.8",
    "react-is": "16.13.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^4",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.9.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  },
  "_moduleAliases": {
    "@econnessione/core": "../../packages/@econnessione/core/lib",
    "@econnessione/shared": "../../packages/@econnessione/shared/lib",
    "@econnessione/ui": "../../packages/@econnessione/ui/lib"
  }
}
