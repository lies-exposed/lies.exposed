{
  "name": "web",
  "version": "0.1.9",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "keywords": [],
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "files": [
    "build",
    "public"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "build:app-server": "pnpm build:client && pnpm build:server-ssr && pnpm build:server",
    "build:client": "vite build --outDir build/client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:server-ssr": "vite build --outDir build/server --ssr src/server/entry.tsx",
    "clean": "rm -rf build",
    "dev": "DOTENV_CONFIG_PATH=.env node --import tsx/esm -r dotenv/config --watch-path=./src ./src/server/server.tsx",
    "docker:dev": "tsx --tsconfig tsconfig.server.json -r dotenv/config --watch-path=./src ./src/server/server.tsx",
    "lint": "eslint src",
    "scripts:make-avatars-mock": "tsx ./scripts/make-avatars-mock.ts",
    "serve": "node ./build/server/server.js",
    "test": "vitest",
    "test:e2e": "vitest --project=web-e2e",
    "test:spec": "vitest --project=web-spec",
    "watch": "tsc -b --watch",
    "watch:client": "DOTENV_CONFIG_PATH=.env.development vite dev"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/io": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@liexp/ui": "workspace:*",
    "@tanstack/react-query": "^5.90.2",
    "@ts-endpoint/react-admin": "file:patches/ts-endpoint-react-admin-0.1.0.tgz",
    "@ts-endpoint/resource-client": "file:patches/ts-endpoint-resource-client-0.1.0.tgz",
    "@ts-endpoint/tanstack-query": "file:patches/ts-endpoint-tanstack-query-0.1.0.tgz",
    "date-fns": "^4.1.0",
    "debug": "^4.4.0",
    "effect": "^3.19.14",
    "express": "^5.2.1",
    "fp-ts": "^2.16.10",
    "path-to-regexp": "^8.3.0",
    "ra-core": "^5.13.4",
    "ra-i18n-polyglot": "^5.13.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-router": "^7.12.0"
  },
  "devDependencies": {
    "@liexp/test": "workspace:*",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.9",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.6",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "msw": "^2.12.7",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-optimizer": "^1.4.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316"
}
