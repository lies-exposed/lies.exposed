    "@visx/gradient": "^2.10.0",
    "@visx/legend": "^2.10.0",
    "@visx/network": "^2.10.0",
    "@visx/responsive": "^2.10.0",
    "@visx/tooltip": "^2.16.0",
    "axios": "^0.27.2",
    "bs58": "^5.0.0",
    "clsx": "^1.2.1",
    "d3": "^7.7.0",
    "debug": "^4.3.4",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "is-plain-object": "^5.0.0",
    "ol": "^6.15.1",
