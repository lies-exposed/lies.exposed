{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "keywords": [],
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "files": [
    "build",
    "public"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "build:app-server": "pnpm build:client && pnpm build:server-ssr && pnpm build:server",
    "build:client": "vite build --outDir build/client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:server-ssr": "vite build --outDir build/server --ssr src/server/entry.tsx",
    "clean": "rm -rf build",
    "dev": "DOTENV_CONFIG_PATH=.env node --import tsx/esm -r dotenv/config ./src/server/server.tsx",
    "docker:dev": "node --import tsx/esm -r dotenv/config ./src/server/server.tsx",
    "lint": "eslint src",
    "scripts:make-avatars-mock": "tsx ./scripts/make-avatars-mock.ts",
    "serve": "node ./build/server/server.js",
    "test": "vitest",
    "watch": "tsc -b --watch",
    "watch:client": "DOTENV_CONFIG_PATH=.env.development vite dev"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "catalog:",
    "@fortawesome/fontawesome-svg-core": "catalog:",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@liexp/ui": "workspace:*",
    "@tanstack/react-query": "catalog:",
    "axios": "catalog:",
    "bs58": "catalog:",
    "clsx": "catalog:",
    "compression": "catalog:",
    "date-fns": "catalog:",
    "debug": "catalog:",
    "express": "catalog:",
    "fp-ts": "catalog:",
    "io-ts": "catalog:",
    "io-ts-types": "catalog:",
    "query-string": "catalog:",
    "ra-core": "catalog:",
    "ra-i18n-polyglot": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-error-boundary": "catalog:",
    "react-router": "catalog:",
    "react-router-dom": "catalog:",
    "sirv": "catalog:"
  },
  "devDependencies": {
    "@types/compression": "catalog:",
    "@types/debug": "catalog:",
    "@types/express": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "dotenv": "catalog:",
    "eslint": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-css-injected-by-js": "catalog:",
    "vite-plugin-optimizer": "catalog:",
    "vite-tsconfig-paths": "catalog:"
  },
  "packageManager": "pnpm@10.2.0+sha512.0d27364e0139c6aadeed65ada153135e0ca96c8da42123bd50047f961339dc7a758fc2e944b428f52be570d1bd3372455c1c65fa2e7aa0bfbf931190f9552001"
}
