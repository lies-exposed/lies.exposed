{
  "name": "worker",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "imports": {
    "#context/*": "./build/context/*",
    "#flows/*": "./build/flows/*",
    "#jobs/*": "./build/jobs/*",
    "#io/*": "./build/io/*",
    "#providers/*": "./build/providers/*",
    "#services/*": "./build/services/*",
    "#utils/*": "./build/utils/*"
  },
  "files": [
    "assets",
    "build",
    "bin",
    "config"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf build",
    "dev": "node --watch build/run.js",
    "dev:alpha": "DOTENV_CONFIG_PATH=.env.alpha pnpm dev",
    "docker:dev": "node --watch build/run.js",
    "lint": "eslint src",
    "bin:run": "node ./build/bin/cli.js",
    "start": "node build/run.js",
    "test": "vitest -r ./ --workspace ./vitest.workspace.js",
    "test:all": "pnpm test:spec && pnpm test:e2e",
    "test:e2e": "vitest -c test/vitest.config.e2e.ts",
    "test:spec": "vitest -c test/vitest.config.spec.ts",
    "tsx:build": "tsx --tsconfig tsconfig.build.json",
    "watch": "tsc -b ./tsconfig.build.json --watch",
    "watch:dev": "tsc -b --watch",
    "pkg": "./scripts/pkg.sh"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "dependencies": {
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "axios": "^1.8.3",
    "cronstrue": "^2.56.0",
    "debug": "^4.4.0",
    "exifreader": "^4.26.2",
    "fluent-ffmpeg": "^2.1.2",
    "fp-ts": "^2.16.9",
    "instagram-private-api": "^1.46.1",
    "io-ts": "^2.2.22",
    "io-ts-types": "^0.5.19",
    "ioredis": "^5.6.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "nodemw": "^0.24.0",
    "page-metadata-parser": "^1.1.4",
    "parse2-kmz": "^1.0.7",
    "pdfjs-dist": "^4.10.38",
    "pg": "^8.14.0",
    "prompts": "^2.4.2",
    "puppeteer-core": "^24.4.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.33.5",
    "typeorm": "^0.3.21",
    "wink-eng-lite-web-model": "^1.8.0",
    "wink-nlp": "^2.3.0",
    "wink-nlp-utils": "^2.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/lodash": "^4.17.16",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/prompts": "^2.4.9",
    "dotenv": "^16.4.7",
    "tsx": "^4.19.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  },
  "packageManager": "pnpm@10.6.2+sha512.47870716bea1572b53df34ad8647b42962bc790ce2bf4562ba0f643237d7302a3d6a8ecef9e4bdfc01d23af1969aa90485d4cebb0b9638fa5ef1daef656f6c1b"
}
