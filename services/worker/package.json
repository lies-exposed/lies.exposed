{
  "name": "worker",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "imports": {
    "#context/*": "./build/context/*",
    "#flows/*": "./build/flows/*",
    "#jobs/*": "./build/jobs/*",
    "#io/*": "./build/io/*",
    "#providers/*": "./build/providers/*",
    "#services/*": "./build/services/*",
    "#utils/*": "./build/utils/*"
  },
  "files": [
    "assets",
    "build",
    "bin",
    "config"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf build",
    "dev": "node --watch build/run.js",
    "docker:dev": "node --watch build/run.js",
    "lint": "eslint src",
    "bin:run": "node ./build/bin/cli.js",
    "bin:generate-thumbnails": "node ./build/bin/cli.js generate-missing-thumbnails",
    "bin:upsert-nlp-entities": "node ./build/bin/cli.js upsert-nlp-entities",
    "start": "node build/run.js",
    "test": "echo 'No tests for worker'",
    "tsx:build": "tsx --tsconfig tsconfig.build.json",
    "watch": "tsc -b ./tsconfig.build.json --watch",
    "watch:dev": "tsc -b --watch",
    "pkg": "./scripts/pkg.sh"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "dependencies": {
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@ts-endpoint/core": "file:patches/ts-endpoint-core-0.1.0.tgz",
    "axios": "^1.12.2",
    "cronstrue": "^3.2.0",
    "debug": "^4.4.0",
    "effect": "^3.17.13",
    "exifreader": "^4.30.0",
    "fluent-ffmpeg": "^2.1.2",
    "fp-ts": "^2.16.10",
    "instagram-private-api": "^1.46.1",
    "ioredis": "^5.8.1",
    "lodash": "^4.17.21",
    "node-cron": "^4.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "nodemw": "^0.25.0",
    "page-metadata-parser": "^1.1.4",
    "parse2-kmz": "^1.0.7",
    "pdfjs-dist": "^5.4.149",
    "pg": "^8.16.0",
    "prompts": "^2.4.2",
    "puppeteer-core": "^24.8.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.34.1",
    "typeorm": "^0.3.27",
    "wink-eng-lite-web-model": "^1.8.0",
    "wink-nlp": "^2.3.0",
    "wink-nlp-utils": "^2.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/lodash": "^4.17.20",
    "@types/node-telegram-bot-api": "^0.64.8",
    "@types/prompts": "^2.4.9",
    "dotenv": "^17.2.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.1",
    "vitest": "^4.0.15"
  },
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402"
}
