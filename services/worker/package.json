{
  "name": "worker",
  "version": "0.1.8",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "imports": {
    "#context/*": "./build/context/*",
    "#flows/*": "./build/flows/*",
    "#jobs/*": "./build/jobs/*",
    "#io/*": "./build/io/*",
    "#providers/*": "./build/providers/*",
    "#services/*": "./build/services/*",
    "#utils/*": "./build/utils/*"
  },
  "files": [
    "assets",
    "build",
    "bin",
    "config"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf build",
    "dev": "node --watch build/run.js",
    "docker:dev": "node --watch build/run.js",
    "lint": "eslint src",
    "bin:run": "node ./build/bin/cli.js",
    "bin:generate-thumbnails": "node ./build/bin/cli.js generate-missing-thumbnails",
    "bin:upsert-nlp-entities": "node ./build/bin/cli.js upsert-nlp-entities",
    "start": "node build/run.js",
    "test": "vitest",
    "test:e2e": "vitest --project worker-e2e",
    "tsx:build": "tsx --tsconfig tsconfig.build.json",
    "watch": "tsc -b ./tsconfig.build.json --watch",
    "watch:dev": "tsc -b --watch",
    "pkg": "./scripts/pkg.sh"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "dependencies": {
    "@liexp/backend": "workspace:*",
    "@liexp/io": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@ts-endpoint/core": "file:patches/ts-endpoint-core-0.1.0.tgz",
    "axios": "^1.13.2",
    "cronstrue": "^3.9.0",
    "debug": "^4.4.0",
    "effect": "^3.19.14",
    "exifreader": "^4.36.0",
    "fluent-ffmpeg": "^2.1.2",
    "fp-ts": "^2.16.10",
    "instagram-private-api": "^1.46.1",
    "ioredis": "^5.9.2",
    "lodash": "^4.17.21",
    "node-cron": "^4.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "nodemw": "^0.25.0",
    "page-metadata-parser": "^1.1.4",
    "parse2-kmz": "^1.0.7",
    "pdfjs-dist": "^5.4.530",
    "pg": "^8.16.0",
    "prompts": "^2.4.2",
    "puppeteer-core": "^24.8.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.34.5",
    "typeorm": "^0.3.28",
    "wink-eng-lite-web-model": "^1.8.0",
    "wink-nlp": "^2.3.0",
    "wink-nlp-utils": "^2.1.0"
  },
  "devDependencies": {
    "@liexp/test": "workspace:*",
    "@types/debug": "^4.1.12",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/lodash": "^4.17.23",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@types/prompts": "^2.4.9",
    "dotenv": "^17.2.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316"
}
