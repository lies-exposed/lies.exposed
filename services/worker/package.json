{
  "name": "worker",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "type": "module",
  "imports": {
    "#context/*": "./build/context/*",
    "#flows/*": "./build/flows/*",
    "#jobs/*": "./build/jobs/*",
    "#io/*": "./build/io/*",
    "#providers/*": "./build/providers/*",
    "#services/*": "./build/services/*",
    "#utils/*": "./build/utils/*"
  },
  "files": [
    "assets",
    "build",
    "bin",
    "config"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf build",
    "dev": "node --watch build/run.js",
    "dev:alpha": "DOTENV_CONFIG_PATH=.env.alpha pnpm dev",
    "docker:dev": "node --watch build/run.js",
    "lint": "eslint src",
    "bin:run": "node ./build/bin/cli.js",
    "start": "node build/run.js",
    "test": "vitest",
    "tsx:build": "tsx --tsconfig tsconfig.build.json",
    "watch": "tsc -b ./tsconfig.build.json --watch",
    "watch:dev": "tsc -b --watch",
    "pkg": "./scripts/pkg.sh"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix"
  },
  "dependencies": {
    "@liexp/backend": "workspace:*",
    "@liexp/core": "workspace:*",
    "@liexp/shared": "workspace:*",
    "@ts-endpoint/core": "file:patches/ts-endpoint-core-0.1.0.tgz",
    "axios": "^1.9.0",
    "cronstrue": "^2.60.0",
    "debug": "^4.4.0",
    "effect": "^3.14.18",
    "exifreader": "^4.30.0",
    "fluent-ffmpeg": "^2.1.2",
    "fp-ts": "^2.16.10",
    "instagram-private-api": "^1.46.1",
    "ioredis": "^5.6.1",
    "lodash": "^4.17.21",
    "node-cron": "^4.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "nodemw": "^0.24.0",
    "page-metadata-parser": "^1.1.4",
    "parse2-kmz": "^1.0.7",
    "pdfjs-dist": "^5.2.133",
    "pg": "^8.16.0",
    "prompts": "^2.4.2",
    "puppeteer-core": "^24.8.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.34.1",
    "typeorm": "^0.3.24",
    "wink-eng-lite-web-model": "^1.8.0",
    "wink-nlp": "^2.3.0",
    "wink-nlp-utils": "^2.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/lodash": "^4.17.20",
    "@types/node-telegram-bot-api": "^0.64.8",
    "@types/prompts": "^2.4.9",
    "dotenv": "^16.4.7",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
