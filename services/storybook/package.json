{
  "name": "storybook",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:ascariandrea/econnessione.git",
  "author": "Andrea Ascari <dev.ascariandrea@gmail.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "storybook dev --quiet -p 6006",
    "build": "tsc -b tsconfig.build.json",
    "build-sb": "storybook build -o build",
    "clean": "rm -rf build",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "serve": "serve -d ./build"
  },
  "dependencies": {
    "@liexp/core": "0.1.0",
    "@liexp/shared": "0.1.0",
    "@liexp/ui": "0.1.0",
    "d3": "^7.9.0",
    "io-ts": "^2.2.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-spring": "^9.7.3",
    "reactflow": "^11.11.3",
    "storybook": "^8.1.2"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": "eslint --fix",
    "src/**/*.+(ts|tsx|js|jsx|json|css)": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.1.2",
    "@storybook/addon-essentials": "^8.1.2",
    "@storybook/addon-interactions": "^8.1.2",
    "@storybook/addon-links": "^8.1.2",
    "@storybook/addon-viewport": "^8.1.2",
    "@storybook/builder-vite": "^8.1.2",
    "@storybook/manager-api": "^8.1.2",
    "@storybook/react": "^8.1.2",
    "@storybook/react-vite": "^8.1.2",
    "@storybook/theming": "^8.1.2",
    "@types/babel__preset-env": "^7.9.6",
    "@types/eslint": "^8.56.10",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "serve": "^14.2.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}
